  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:ui": "pnpm --filter pulse-connect-ui dev",
    "dev:admin": "pnpm --filter pulse-connect-admin-ui dev",
    "dev:core": "pnpm --filter pulse-connect-core dev",
    "dev:pap": "pnpm --filter pap_v1 dev",
    "build": "pnpm -r build",
    "build:ui": "pnpm --filter pulse-connect-ui build",
    "build:admin": "pnpm --filter pulse-connect-admin-ui build",
    "build:core": "pnpm --filter pulse-connect-core build",
    "build:pap": "pnpm --filter pap_v1 build",
    "start": "pnpm -r --parallel start",
    "start:ui": "pnpm --filter pulse-connect-ui start",
    "start:admin": "pnpm --filter pulse-connect-admin-ui start",
    "start:core": "pnpm --filter pulse-connect-core start",
    "start:pap": "pnpm --filter pap_v1 start",
    "test": "pnpm -r test",
    "test:ui": "pnpm --filter pulse-connect-ui test",
    "test:admin": "pnpm --filter pulse-connect-admin-ui test",
    "test:core": "pnpm --filter pulse-connect-core test",
    "test:pap": "pnpm --filter pap_v1 test",
    "test:protocol": "pnpm --filter Pulse-Agent-Protocol test",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "typecheck": "pnpm -r typecheck",
    "format": "pnpm -r format",
    "security": "pnpm -r security",
    "clean": "pnpm -r clean",
    "install:all": "pnpm install",
    "postinstall": "husky install",
    "prepare": "husky install",
    "ci": "pnpm install --frozen-lockfile && pnpm lint && pnpm typecheck && pnpm test && pnpm build",
    "deploy": "pnpm build && pnpm -r deploy",
    "deploy:staging": "pnpm build && pnpm -r deploy:staging",
    "deploy:production": "pnpm build && pnpm -r deploy:production",
    "docker:build": "docker build -t pulsco-ecosystem .",
    "docker:run": "docker run -p 3000:3000 pulsco-ecosystem",
    "docker:compose": "docker-compose up -d",
    "db:migrate": "pnpm --filter pap_v1 db:migrate",
    "db:seed": "pnpm --filter pap_v1 db:seed",
    "db:reset": "pnpm --filter pap_v1 db:reset",
    "proximity:health": "pnpm --filter pulse-connect-core proximity:health",
    "pap:campaigns": "pnpm --filter pap_v1 campaigns:list",
    "monitoring": "pnpm --filter pulse-connect-core monitoring:start",
    "logs": "pnpm -r logs",
    "backup": "pnpm -r backup",
    "celebrate": "node scripts/triggerCelebration.js"
  },
